<Schema name="contaminacion">
	<Cube name="contaminacion">
		<Table name="contaminacion"/>

		<Dimension name="fabrica" >
			<Hierarchy name="Categoria" hasAll="true">

				<Level name="fabrica_fabrica_latLon" type="Numeric" column="fabrica_fabrica_latLon">
					<Property name="fabrica_cantEmple" column="fabrica_cantEmple" type="Numeric"/>
				</Level>

				<Level name="fabrica_empresa_nombre" type="String" column="fabrica_empresa_nombre">
					<Property name="fabrica_empresa_factAnual" column="fabrica_empresa_factAnual" type="Numeric"/>
				</Level>
				
				<Level name="fabrica_rubro_tipoID" type="Numeric" column="fabrica_rubro_tipoID">
					<Property name="fabrica_rubro_tipoDesc" column="fabrica_rubro_tipoDesc" type="String"/>
				</Level>

			</Hierarchy>
			
			<Hierarchy name="geoLocation" hasAll="true">
				
				<Level name="fabrica_fabrica_latLon" type="Numeric" column="fabrica_fabrica_latLon">
					<Property name="fabrica_cantEmple" column="fabrica_cantEmple" type="Numeric"/>
				</Level>
			
				<Level name="fabrica_ciudad_nombre" column="fabrica_ciudad_nombre" type="String">
					<Property name="fabrica_ciudad_poblacion" column="fabrica_ciudad_poblacion" type="Numeric"/>
				</Level>
				
				<Level name="fabrica_pais_nombre" type="String" column="fabrica_pais_nombre">
					<Property name="fabrica_pais_the_geom" column="fabrica_pais_the_geom" type="Integer"/>
				</Level>
				
			</Hierarchy>
		
		</Dimension>

		<Dimension name="inicio">
			<Hierarchy name="calendar" hasAll="true">

				<Level name="inicio_temporal_dia" column="inicio_temporal_dia" type="Timestamp"/>

				<Level name="inicio_month_mes" column="inicio_month_mes" type="String" >
					<Property name="inicio_month_nombre" column="inicio_month_nombre" type="String"/>
				</Level>
			
				<Level name="inicio_year_anio" type="String" column="inicio_year_anio">
					<Property name="inicio_year_esBisiesto" column="inicio_year_esBisiesto" type="Numeric"/>
				</Level>
	
			</Hierarchy>
		</Dimension>

		<Dimension name="fin">
			<Hierarchy name="calendar" hasAll="true">

				<Level name="fin_temporal_dia" column="fin_temporal_dia" type="Timestamp"/>

				<Level name="fin_month_mes" column="fin_month_mes" type="String" >
					<Property name="fin_month_nombre" column="fin_month_nombre" type="String"/>
				</Level>
			
				<Level name="fin_year_anio" type="String" column="fin_year_anio">
					<Property name="fin_year_esBisiesto" column="fin_year_esBisiesto" type="Numeric"/>
				</Level>
	
			</Hierarchy>
		</Dimension>

		<Dimension name="sustanciaDim">
			<Hierarchy name="sustancia" hasAll="true">

				<Level name="sustanciaDim_sustancia_denominacion" column="sustanciaDim_sustancia_denominacion" type="String"/>
	
			</Hierarchy>
		</Dimension>

		
		<Measure name="emision" column="emision" aggregator="sum" datatype="Numeric"/>
		<Measure name="emisionmala" column="emisionMala" aggregator="avg" datatype="Numeric"/>
		<Measure name="nube" column="nube" aggregator="min" datatype="Integer"/>

	</Cube>
	
</Schema>
